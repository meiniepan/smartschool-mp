/* packageA/pages/addLog/addLog.wxss */
page{
    background: var(--tintBac);
}
.remark{
    display: flex;
    width: 100%;
    height: 150rpx;
    padding: 20rpx;
    background: var(--white);
    box-sizing: border-box;
}
.bottom{
    position: fixed;
    width: 100%;
    bottom: 0;
    display: flex;
    flex-direction: row-reverse;
    height: 84rpx;
    background: var(--grayF6);
}
.confirm{
    display: flex;
    width: 172rpx;
    height: 100%;
    background: var(--themeBac);
    color: var(--white);
    align-items: center;
    justify-content: center;
}