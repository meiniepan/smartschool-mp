/* pages/quantizeSpecialList/quantizeSpecialList.wxss */
page{
    background: var(--tintBac);
}
.cells{
    display: flex;
    flex-direction: column;
    margin-top: 24rpx;
}
.cell{
    display: flex;
    flex-direction: column;
    margin-bottom: 20rpx;
    background: var(--white);
    padding: 30rpx var(--titlePadding);
}
.cells0{
    display: flex;
    width: 100%;
    flex-direction: column;
    box-sizing: border-box;
}
.item0 {
    display: flex;
    flex-direction: row;
    align-items: center;
}
.title-area{
    display: flex;
    justify-content: space-between;
}
.remark-area{
    display: flex;
    flex-direction: column;
}
.item{
    display: flex;
    flex-direction: row;
    align-items: center;
    padding: 30rpx var(--titlePadding);
    border-bottom: 1rpx solid var(--splitColor);
}
.item-remark{
    display: flex;
    flex-direction: column;
    padding: 30rpx var(--titlePadding);
    border-bottom: 1rpx solid var(--splitColor);
}
.key{
    font-size: 30rpx;
    font-weight: bold;
}
.value{
    font-size: 28rpx;
    color: var(--commonHint);
    margin-left: 10rpx;
}
.value-remark{
    font-size: 28rpx;
    color: var(--commonHint);
}