/* packageA/pages/eduAttendanceDetail/eduAttendanceDetail.wxss */
page {
    background: var(--tintBac);
}

.top-area {
    display: flex;
    flex-direction: column;
    margin: 20rpx;
    padding: 20rpx 40rpx;
    box-sizing: border-box;
    background: var(--white);
    border-radius: 32rpx;
}
.head-line{
    display: flex;
    justify-content: space-between;
}

.cells {
    padding: 40rpx var(--titlePadding) 0rpx;
}

.cell {
    display: flex;
    flex-direction: column;
    background-color: white;
    padding: 40rpx;
    border-radius: 12rpx;
    margin-bottom: 20rpx;
}

.titles {
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.title-name {

}

.title {
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    text-overflow: ellipsis;
    overflow: hidden;
}