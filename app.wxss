/**app.wxss**/

page {
  --userToken: 'userToken';


  --themeColor: #445FF5;
  --commonBlack: #222222;
  --divider-line-color: #cecece;
  --divider-line-color-dark: #5d5d5f;
  --tags-font-size: 25rpx;
  --tags-color: #777777;
  --text-color: #fff;
  --text-color-dark: #a9a9b2;
  --page-backgroung-color: #F7F7F7;
  --page-backgroung-color-dark: #343538;
  --cell-background-color: #fff;
  --login-header-color: #48C9B0;

  --themeBac: linear-gradient(to right,#445FF5,#6D0BBD);

  font-size: 32rpx;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
