/**app.wxss**/

page {
  --userToken: 'userToken';


  --themeColor: #445FF5;
  --tintBac: #F5F7FD;
  --commonBac: #ffffff;
  --white: #ffffff;
  --splitColor: #E3E6E9;
  --commonBlack: #222222;
  --commonHint: #999999;
  --grayF6: #F6F6F6;

  --themeBac: linear-gradient(to right,#445FF5,#6D0BBD);
  --titlePadding: 46rpx;

  font-size: 32rpx;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
