/* pages/my_code/my_code.wxss */
page {
    background-color: var(--tintBac);
}
.cells{

    margin-top: 20rpx;
    margin-left: var(--titlePadding);
    margin-right: var(--titlePadding);
}
.cell{
    width: calc(329*2rpx);
    height: calc(246*2rpx);
    margin-bottom: 124rpx;
    position: relative;
}
.image{
    width: 100%;
    height: 100%;
}
.out2{
    width: 100%;
    height: 100%;
    box-sizing:border-box;
    position: absolute;
    top: 0;
    left: 0;
    display: flex;
    flex-direction: column;
    justify-content: center;
}
.name{
    margin: 0 auto;
    text-align: center;
    font-size: 32rpx;
}
.code{
    margin: 0 auto;
    display: flex;
    flex-direction: column;
    justify-content: center;
    width: 400rpx;
    height: 100rpx;
    background: var(--grayF6);
    text-align: center;
margin-top: 28rpx;
    font-size: 40rpx;
}
.copy{
    margin: 0 auto;
    text-align: center;
margin-top: 52rpx;
    font-size: 32rpx;
    color: var(--themeColor);
    border-bottom: var(--themeColor) solid 1rpx;
}
