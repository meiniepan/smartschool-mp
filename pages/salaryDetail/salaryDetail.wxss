/* pages/salaryDetail/salaryDetail.wxss */
page{
    background: var(--tintBac);
}
.out{
    width: 100vw;
    position: relative;
    display: flex;
    flex-direction: column;
}
.out1 {
    width: 100%;
    height: 368rpx;
    display: flex;
    flex-direction: column;
    padding: 0 var(--titlePadding);
    background: var(--themeBac);
}
.title1 {
    margin-top: 40rpx;
    font-size: 60rpx;
    font-weight: bold;
    color: var(--white);
}

.title2 {
    margin-top: 0rpx;
    font-size: 24rpx;
    color: var(--white);
}
.cells{
    width: clac(100vw - 46rpx);
    display: flex;
    flex-direction: column;
    position: absolute;
    margin-top: 288rpx;
    margin-bottom: 40rpx;
    margin-left: var(--titlePadding);
    margin-right: var(--titlePadding);
    padding-bottom: 50rpx;
    background: var(--white);
    border-radius: 12rpx;
    box-sizing: border-box;
}
.cell{

}
.cell:not(:last-child) {

}
.ext{
    display: flex;
    padding:30rpx 22rpx;
    justify-content: space-between;
    align-items: center;
    border-bottom: var(--splitColor) solid 1rpx;
}
.ext-key{
    font-size: 28rpx;
    color: var(--commonHint);
}
.ext-value{
    font-size: 40rpx;
    font-weight: bold;
    color: var(--commonBlack);
}