/* pages/repairList/repairList.wxss */
page {
    background: var(--tintBac);
}

.cells {
    padding-top: 20rpx;
}

.cell {
    display: flex;
    flex-direction: column;
    background-color: white;
    padding: 40rpx var(--titlePadding);
    margin-bottom: 20rpx;
}

.titles {
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-bottom: 20rpx;
}

.title-name {
    font-size: 30rpx;
    font-weight: bold;
    min-width: 150rpx;
}

.title {
    display: -webkit-box;
    -webkit-box-orient: vertical;
    color: var(--commonHint);
    -webkit-line-clamp: 1;
    text-overflow: ellipsis;
    overflow: hidden;
}
.under-line {
    border-bottom: var(--themeColor) solid 1rpx;
}

.remark {
    color: var(--commonHint);
}

.cells-image {
    display: flex;
    justify-content: space-around;
    flex-wrap: wrap;
    margin-bottom: 20rpx;
}

.image {
    width: 200rpx;
    height: 200rpx;
    margin-top: 14rpx;
    border-radius: 12rpx;
}